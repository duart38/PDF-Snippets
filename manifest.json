{
    "name": "Sectioned PDF",
    "action": {
        "default_icon": {
            "16": "/icons/16.png",
            "32": "/icons/32.png",
            "64": "/icons/64.png",
            "128": "/icons/128.png",
            "256": "/icons/256.png"
          }
    },
    "manifest_version": 3,
    "version": "0.1",
    "description": "Turns a portion (element) of a webpage into PDF",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "src/bg/background.js"
    }
}