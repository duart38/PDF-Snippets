{
    "name": "PDF Snippets",
    "action": {
        "default_icon": {
            "16": "/icons/16.png",
            "32": "/icons/32.png",
            "64": "/icons/64.png",
            "128": "/icons/128.png",
            "256": "/icons/256.png"
          }
    },
    "manifest_version": 3,
    "version": "0.2",
    "description": "Extract a portion / section of a webpage into a PDF file",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "src/bg/background.js"
    }
}